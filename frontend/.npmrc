# NPM optimization for Docker builds
audit=false
fund=false
progress=false
prefer-offline=true
cache-max=86400000
package-lock=true

# Production optimizations
production=false
optional=false
dev=false

# Performance optimizations
maxsockets=20
fetch-retries=2
fetch-retry-factor=10
fetch-retry-mintimeout=10000
fetch-retry-maxtimeout=60000