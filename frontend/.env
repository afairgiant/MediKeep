# React Environment Variables
# API URL for the FastAPI backend
REACT_APP_API_URL=http://localhost:8000/api/v1

# App Name
REACT_APP_NAME=Medical Records System

# Debug mode
REACT_APP_DEBUG=true

# Disable host checking to avoid VPN interface issues
DANGEROUSLY_DISABLE_HOST_CHECK=true
PORT=3000


# ESLint configuration for CI builds
# Disable treating warnings as errors in production builds
ESLINT_NO_DEV_ERRORS=true
DISABLE_ESLINT_PLUGIN=false

# Build Configuration
ESLINT_NO_DEV_ERRORS=true
GENERATE_SOURCEMAP=false